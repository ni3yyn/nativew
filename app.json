{
  "expo": {
    "name": "Wathiq",
    "slug": "nativew",
    "scheme": "wathiq",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A2D27"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.app.wathiq",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.kousaila.wathiq",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1A2D27"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Wathiq to access your photos to scan ingredients.",
          "cameraPermission": "Allow Wathiq to access your camera to scan ingredients."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6ebdbfe1-08ea-4f21-9fa2-482f152a3266"
      }
    }
  }
}
