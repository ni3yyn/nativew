{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "ndk": {
          "abiFilters": ["arm64-v8a"]
        }
      }
    },
    "preview": {
      "android": {
        "buildType": "apk" ,
        "gradleCommand": ":app:assembleRelease --stacktrace --info"
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}